{
  "name": "prawr-server",
  "version": "0.1.0",
  "description": "The server for prawr",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "mongoadmin": "mongo --host 127.0.0.1:27017"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Prawr/Prawr-Server.git"
  },
  "keywords": [
    "node",
    "restful",
    "api"
  ],
  "jest": {
    "coverageDirectory": "coverage",
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/**/*",
      "<rootDir>/tests/**/*"
    ],
    "collectCoverageFrom": [
      "<rootDir>/api/**/*.{js,jsx,mjs}"
    ],
    "testMatch": [
      "<rootDir>/tests/**/*"
    ]
  },
  "author": "https://github.com/Prawr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Prawr/Prawr-Server/issues"
  },
  "homepage": "https://github.com/Prawr/Prawr-Server#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "http": "0.0.0",
    "mongoose": "^4.13.7",
    "morgan": "^1.9.0",
    "password-hash": "^1.2.2"
  },
  "devDependencies": {
    "jest": "^22.0.4",
    "nodemon": "^1.14.3",
    "supertest": "^3.0.0"
  }
}
